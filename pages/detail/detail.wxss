}
.pro-detail-images image{
  max-width: 100%;
  vertical-align:middle;
}
.pro-detail-rule{
  padding: 0 30rpx;
  text-align: center;
  margin-top: 40rpx;
}
.rule-pic{
  width: 692rpx;
  height: 421rpx;
}
.bid-price-pic{
  width: 690rpx;
  height: 662rpx;
  margin-bottom:30rpx;
}
.rule-name{
  margin-top: 30rpx;
  font-family: 'PingFang-SC-Medium';
