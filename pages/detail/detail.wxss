/* 出价弹框 */
.bids-box{
  width: 100%;
  height: 830rpx;  
  overflow: hidden;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 3001;
  background: #fff;
}
.bids-detail{
  padding: 40rpx 30rpx 0;
  display: flex;
  justify-content: space-between;
}
.first-pic{
  width: 200rpx;
  height: 200rpx;
  background: #FFFFFF;
  border: 1rpx solid #DDDDDD;
}
.first-pic image{
  width: 100%;
  height: 100%;
  margin: 0 auto;
}
.bids-name{
  max-width: 400rpx;
  flex: 1;
}
.start-price2{
  font-size: 26px;
}
.close-icon{
  width: 28rpx;
  height: 28rpx;
  margin-top: 26rpx;
}
.bids-title{
  width: 100%;
	white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  height: 60rpx;
  font-family: 'PingFang-SC-Medium';
  color: #333333;
  font-size: 14px;
  margin: 20rpx 0 50rpx;
}
.bids-price{
  color: #333333;
  font-family: 'PingFang SC';
  font-size: 14px;
}
.bids-money{
  padding: 0 30rpx;
  display: flex;
  height: 190rpx;
  font-size: 15px;
  font-family: 'PingFang-SC-Regular';
  color: #333333;
  justify-content: space-between;
  align-items: center;
}
.bids-number{
  display: flex;
  width: 360rpx;
}
.input-price{
  width: 180rpx;
  height: 72rpx;
  text-align: center;
  border: 1rpx solid #757575;
  font-size: 16px;
}
.min{
  display: inline-block;
	width: 90rpx;
	height: 72rpx;
  text-align:center;
  line-height: 72rpx;
	vertical-align: middle;
  border: 1rpx solid #DDDDDD;
  background: url(https://static.pecoo.com/images/detail/reduce.png) no-repeat center center;
  border-right: none;
  background-size: 26rpx;
  font-size: 20px;
  color: #DDDDDD;
}
.colour{
  background: url(https://static.pecoo.com/images/detail/reduce_selected.png) no-repeat center center;
  border-color: #757575;
  background-size: 26rpx;
}
.add{
  display: inline-block;
	width: 90rpx;
	height: 72rpx;
  text-align:center;
  line-height: 72rpx;
	vertical-align: middle;
  border: 1rpx solid #757575;
  border-left: none;
  font-size: 20px;
  background: url(https://static.pecoo.com/images/detail/add_selected.png) no-repeat center center;
  background-size: 26rpx;
}
.bids-total{
  padding: 0 30rpx;
  height: 170rpx;
	font-size: 16px;
	line-height: 170rpx;
  position: relative;
}
.total-price{
  float: right;
  font-family: 'PingFang SC';
  color: #333333;
  font-size: 16px;
}
.cont{
  position: absolute;
  height: 24rpx;
  line-height: 24rpx;
  font-family: 'PingFang-SC-Regular';
  font-size: 12px;
  color: #999999;
  bottom: 0;
  right: 30rpx;
}
.yes{
	position: absolute;
	bottom: 0;
  
	height: 110rpx;
	width: 100%;
	background: #1B1B1B;
	color: #FFFFFF;
	font-size: 17px;
	text-align: center;
	line-height: 110rpx;
}
.tips{
  font-size: 12px;
  font-family: 'PingFangSC-Regular';
  color: #E83D3E;
  padding: 20rpx 30rpx;
}
/* 弹出框的title和起拍价 */
.sug-box{
  position: fixed;
  width:100%;
  height: auto;
  left:0%;
  bottom:0px;
  background: #fff;
  font-size: 18px;
  text-align:center;
  line-height: 20px;
  z-index: 3001;
}
.sug-box>view:first-child{
  font-size: 13px;
  color: #999999;
  display: flex;
  align-items: center;
  font-family: 'PingFang SC';
  height: 109rpx;
  padding:0 64rpx;
  border-bottom: 1rpx solid #E2E2E2;
}
.item-price{
  height: 100rpx;
  line-height: 100rpx;
  font-size: 20px;
  color: #333333;
}
.sug-box>view:last-child{
  height: 130rpx;
  display: flex;
  align-items: center;
}
.item-suggest-price{
  background: #F4F4F4;
}
.cancel{
  margin: 0 30rpx;
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  background: #1B1B1B;
  color: #FFFFFF;
  font-size: 17px;
}
