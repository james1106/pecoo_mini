}
/* 底部 ma */
.detailFix{
	background: #FFFFFF;
	height: 130rpx;
	position: fixed;
	bottom: 0;
	width: 100%;
	display: -webkit-box;
	font-size: 12px;
	color: #333333;
  font-family: 'PingFang-SC-Regular';
  box-shadow:8px 0px 13px rgba(51,51,51,0.06);
}
.detail-btn{
  margin: 20rpx 0;
	width:100rpx;
  display: flex;
  height: 90rpx;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.detailFix button{
  border: none;
  background: #FFFFFF
}
.detail-btn image{
	display: block;
	width: 22px;
	height: 22px;
	margin:0 auto;
}
.detailFix button{
	background: #1B1B1B;
	-webkit-box-flex: 1.0;
  margin:20rpx 30rpx 0 30rpx;
	text-align: center;
  height: 90rpx;
	line-height: 90rpx;
	color: #FFFFFF;
  border-radius: 0;
	font-size: 15px;
}
