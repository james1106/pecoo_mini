.open-app{
  margin-top: 80rpx;
}
.swiperTime{
	position: absolute;
	top:0;
	z-index: 200;
	background: rgba(255,255,255,.6);
	width: 100%;
	height: 25px;
	color: #DB1014;
	font-size: 14px;
	text-align: center;
	line-height: 25px;
}
.swiper-container {
  width: 100%;
  position: relative;
}
swiper {
  text-align: center;
  font-size: 18px;
  background: #fff;
  display: -webkit-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  -webkit-align-items: center;
  align-items: center;
  width:100%;
  height: 330px;
}
swiper image{
  width: 100%;
  height: 650rpx;
	max-width:100%;
	max-height: 650rpx;
}
.swiper-pagination-bullet{
	background: #666;
	width:5px;
	height:5px;
}
.swiper-container-horizontal>.swiper-pagination {
  bottom: 10px;
  left: 0;
  width: 100%;
  height: 15px;
  overflow: hidden;
}
.pro-detail-notice{
  margin: 60rpx 30rpx 0;
  line-height:110rpx;
  border-top:1rpx solid #EEEEEE;
  text-align: center;
  color: #333333;
  font-size: 16px;
  font-family: 'PingFang-SC-Medium';
}
.pro-detail-cont{
  padding: 0 30rpx;
  font-family: 'PingFang-SC-Regular';
  font-size: 13px;
  color: #333333;
  text-indent: 28px;
}
/*商品详情*/
.product-detail{
	box-sizing: border-box;
	background: #FFFFFF;
	padding: 50rpx 30rpx 47rpx;
}
.hengP{
	height: 10px;
	background: #F0EFF7;
	width: 100%;
}
.pro-price-box{
  display: flex;
  justify-content: space-between;
}
.pro-price{
  font-family: 'Avenir';
  color: #333333;
  font-size: 14px;
  width: 80%;
}
.price-number{
  font-size: 26px;
  margin-right: 24rpx;
  font-family: 'Avenir';
}
.original-price{
  text-decoration:line-through;
  color: #999999;
  font-size: 13px;
}
.pro-collect{
  width:50rpx;
  height:50rpx;
  float: right;
  vertical-align: middle;
}
.start-price{
  background:#E83E3E;
  color: white;
  font-family: 'PingFang-SC-Regular';
  font-size: 10px;
  height:30rpx;
  width: 74rpx;
  line-height: 30rpx;
  text-align: center;
  margin-bottom: 15rpx;
}
.estimate{
  font-family: 'PingFang-SC-Regular';
  display: flex;
  justify-content: space-between;
}
.estimate:nth-child(1){
  font-size: 13px;
  color: #333333;
}
.estimate:nth-child(3){
  font-family: 'PingFang-SC-Regular';
  font-size: 13px;
  color: #999999;
}
.estimate-offer{
  padding-left: 8rpx;
  color: #333333;
}
.pro-name{
  font-family: 'PingFang-SC-Medium';
  color: #333333;
  font-size: 20px;
  margin: 30rpx 0 24rpx;
}
.time-icon{
	width: 12px;
  height: 12px;
	vertical-align: middle;
	margin-right: 5px;
}
.pro-public{
  font-family: 'PingFang-SC-Regular';
  color: #666666;
  font-size: 13px;
  padding-bottom: 4rpx;
  display: flex;
  align-items: center;
}
.pro-auction{
  margin-top: -8rpx;
  font-family: 'PingFang-SC-Regular';
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.pro-auction navigator{
	text-decoration: underline;
  color:#1B1B1B;
  font-size: 13px;
}
/* 拍品详情 */
.pro-detail-box{
  padding:0 30rpx;
}
.pro-detail-name{
  text-align:center;
