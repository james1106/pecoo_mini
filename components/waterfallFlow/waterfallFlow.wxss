.fall-container {
  width: 100%;
  height: auto;
  color: #333333;
  display: flex;
}
.fall-left {
  width: 50%;
  display: flex;
  height: 100%;
  flex-direction: column;
}
.item-box{
  background: #FFFFFF;
  box-shadow: 0 1px 5px #DDDDDD;
  border-radius: 10rpx;
  font-family: 'PingFang-SC-Regular';
}
.fall-right {
  width: 50%;
  display: flex;
  flex-direction: column;
}
.warp{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.item-content image{
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  max-width: 100%;
}
.goods-name{
  font-size: 14px;
  padding: 10rpx 26rpx 0 26rpx;
}
.start-time{
  color: #666666;
  font-size: 11px;
  padding: 6rpx 0 0 26rpx;
}
.goods-price{
  font-size: 12px;
  padding: 15rpx 0 20rpx 20rpx;
}
.start-price{
  font-size: 18px;
  color: #333333;
  font-family: 'Arial';
  padding-left: 8rpx;
}
