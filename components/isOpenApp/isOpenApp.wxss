.solid{
  width: 100%;
  height: 1px;
  background: #F1F1F1;
  position: fixed;
  top: 0;
  left:0;
  z-index: 11111;
}
.open-app{
  width: 100%;
  height: 80rpx;
  background: #F7F7F7 !important;
  display: flex;
  justify-content: space-between;
  font-size: 12px;
  align-items: center;
  position: fixed;
  top: 1px;
  left: 0;
  z-index: 11111;
}
.open-app-title{
  padding-left: 18rpx;
}
.open-app-content{
  width: 70%;
  margin-left: 20rpx;
  display: flex;
  align-items: center;
}
.open-app-content image{
  max-width: 49rpx;
  max-height: 49rpx;
}
.click-open{
  color: #E83E3E !important;
  border:none !important;
  border-radius: 0 !important;
  font-size: 12px;
}