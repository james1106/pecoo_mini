.order-pro{
  width: 100%;
  min-height: 160rpx;
  border-top: 1px solid #E9E9E9;
  background: #FAFAFA;
  display: flex;
  align-items: center;
  font-size: 14px;
  padding: 20rpx 0;
  color: #999999;
  font-family: 'HiraginoSansGB-W3';
}
.pro-pic-box{
  width: 180rpx;
  height: 180rpx;
  margin-left: 30rpx;  
  border: 1px solid #CCCCCC;
}
.pro-pic-box image{
  width: 100%;
  height: 100%;
  margin: 0 auto;
}
.pro-detail{
  flex: 1;
  padding: 0 30rpx;
}
.pro-name{
  max-width: 500rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  color: #333333;
  font-size: 15px;
  padding: 10rpx 0;
}
.pro-detail>view{
  font-size: 14px;
}